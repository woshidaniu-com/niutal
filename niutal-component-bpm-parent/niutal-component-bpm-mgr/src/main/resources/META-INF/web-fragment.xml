<web-fragment version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd">

	<!-- 通过<name>元素来指定当前模块的名称 -->
	<name>niutal-bpm</name>
    
	 <filter>
	     <filter-name>JSONPFilter</filter-name>
	     <filter-class>com.woshidaniu.component.bpm.web.JsonpCallbackFilter</filter-class>
	 </filter>
	
	 <filter-mapping>
	     <filter-name>JSONPFilter</filter-name>
	     <url-pattern>/processDiagram/*</url-pattern>
	 </filter-mapping>
	 <filter-mapping>
	     <filter-name>JSONPFilter</filter-name>
	     <url-pattern>/activiti-rest-service/*</url-pattern>
	 </filter-mapping>
	
	
</web-fragment> 