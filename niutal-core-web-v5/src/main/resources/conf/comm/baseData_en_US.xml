<?xml version="1.0" encoding="UTF-8"?>
<!-- 基础数据XML,修改后请重新启动服务器 -->
<root>

	<!-- 系统支持语言-->
	<data id="languageList">
		<option key="zh_CN">Chinese</option>
		<option key="en_US">English</option>
	</data>

	<!-- 新闻发布中的发布对象 -->
	<data id="fbdx">
		<option key="tea">All Teachers</option>
		<option key="stu">All Students</option>
	</data>
	<!-- 新闻发布阅读权限 -->
	<data id="ydqx">
		<option key="auth">Authenticate</option>
		<option key="public">Public</option>
	</data>
	<!-- 就业服务类型 -->
	<data id="jyfwlx">
		<option key="1">Career Talk</option>
		<option key="2">Career Fair</option>
		<option key="3">Written Exam</option>
		<option key="4">Oral Quiz</option>
	</data>
	
	<!-- 状态标签 -->
	<data id="isStart">
		<option key="1">Enable</option>
		<option key="0">Disable</option>
	</data>

	<!-- 是/否标签 -->
	<data id="isNot">
		<option key="1">Yes</option>
		<option key="0">No</option>
	</data>
	
	<!-- 非公用/公用标签 -->
	<data id="isPublic">
		<option key="1">Public</option>
		<option key="0">Non-Public</option>
	</data>
	
	<!-- 允许/不允许标签 -->
	<data id="isAllow">
		<option key="1">Allowed</option>
		<option key="0">Unallowed</option>
	</data>
	
	<!-- 允许/不允许标签 -->
	<data id="isValid">
		<option key="1">Valid</option>
		<option key="0">Invalid</option>
	</data>
	<!-- 是否可录入 -->
	<data id="isSfklr">
		<option key="0">Allow Input</option>
		<option key="1">Forbid Input</option>
	</data>
	<!-- 有无分数段 -->
	<data id="isYffsd">
		<option key="1">Exists</option>
		<option key="0">Not Exists</option>
	</data>
	<!-- 是否参加计算绩点 -->
	<data id="isScjjdjs">
		<option key="1">Contribute</option>
		<option key="0">Not Contribute</option>
	</data>
	<!-- 录入方式(1输入;2选择)-->
	<data id="isLrfs">
		<option key="2">Dropdown List Selection</option>
		<option key="1">Textbox Input</option>
	</data>
	<!-- 排课类别 -->
	<data id="pklbList">
	 	<option key="1">Institute</option>
	 	<option key="2">Major</option>
	 	<option key="3">Course</option>
	 	<option key="4">Teacher</option>
	 	<option key="5">Classroom</option>
	 	<option key="6">Grade</option>
	 	<option key="7">Class</option>
	 	<option key="8">Grade Major</option>
	 	<option key="9">Campus</option>
	 	<!--<option key="10">Student</option>:此记录在其他选课规则个人选课控制中使用；请勿删除-->
	</data>
	
	<!-- 成绩导入方式(1追加 ;2覆盖)-->
	<data id="cjDrfsList">
		<option key="1">Append</option>
		<option key="2">Overwrite</option>
	</data>
	
	<!-- Excel后缀-->
	<data id="excelSuffixList">
		<option key="XLS">*.xls</option>
		<option key="XLSX">*.xlsx</option>
	</data>
	
	<!-- 否视为被使用-->
	<data id="canUsed">
		<option key="1">Regard as Using</option>
		<option key="0">Regard as Not Used</option>
	</data>
	
	<!-- 能否修改-->
	<data id="canModify">
		<option key="1">Modifiable</option>
		<option key="0">Non-Modifiable</option>
	</data>
	
	<!-- 排考试方式-->
	<data id="pkMethod">
		<option key="1">Arrange Exam Before Curricula-Variable</option>
		<option key="2">Arrange Exam After Curricula-Variable</option>
	</data>
	
	<!-- 加密进度-->
	<data id="encryptStatus">
		<option key="1">Unencrypted </option>
		<option key="2">Partial Encryption </option>
		<option key="3">All Encryption </option>
	</data>
	
	<!-- 处理进度-->
	<data id="processStatus">
		<option key="1">Unprocessed</option>
		<option key="2">Partial Processing </option>
		<option key="3">All Processed</option>
	</data>
	
	<!-- 设置进度 -->
	<data id="settingList">
		<option key="-1">Not Set</option>
		<option key="0">Partial Setting </option>
		<option key="1">Already Set </option>
	</data>
	
	<!-- 设置状态 -->
	<data id="settingStatus">
		<option key="0">Not Set</option>
		<option key="1">Already Set</option>
	</data>
	
	<!-- 修改方式-->
	<data id="xgfsList">
		<option key="1">Replace</option>
		<option key="2">Prefix</option>
		<option key="3">Suffix</option>
	</data>
	
	<!-- 照片格式 -->
	<data id="zpgsList">
		<option key="jpg">JGP</option>
		<option key="jpeg">JPEG</option>
		<option key="gif">GIF</option>
		<option key="png">PNG</option>
	</data>
	
	<!-- 选必修-->
	<data id="xbx">
		<option key="bx">Compulsory</option>
		<option key="xx">Elective</option>
	</data>	
	
	<!-- 评价显示分类 -->
	<data id="pjlxList">
		<option key="01">Students Evaluation</option>
		<option key="02">Questionnaire Survey</option>
	</data>	
	
	<!-- 评价显示分类 -->
	<data id="pjxsflList">
		<option key="1">By Courses</option>
		<option key="2">By Institutes</option>
	</data>	
	
	<!-- 评分类型 -->
	<data id="pflxList">
		<option key="1">Index</option>
		<option key="2">Score</option>
	</data>	
	
	<!-- 答题类型 -->
	<data id="dtlxList">
		<option key="1">Subjective Tests</option>
		<option key="2">Objective Tests</option>
	</data>	
	
	<!-- 自定义功能：功能类型 -->
	<data id="functypeList">
		<option key="1">Data Display</option>
		<option key="2">Data Maintenance </option>
		<option key="3">Print Preview </option>
		<option key="4">Quick Print</option>
		<option key="5">Data Export </option>
		<option key="6">Data Deletion</option>
	</data>	
	
	<!-- 自定义功能：字段值来源;-->
	<data id="sourceTypeList">
		<option key="APP">Program Built in </option>
		<option key="SQL">Database</option>
		<option key="XML">XML Data</option>
		<option key="Spring">Spring Collection Objects</option>
		<option key="Fixed">Fixed Values</option>
		<option key="Base">Basic Data</option>
	</data>	
	
	<!-- 自定义功能：字段使用范围;-->
	<data id="fieldScopeList">
		<option key="1">Whole Situation</option>
		<option key="2">Framework</option>
		<option key="3">Localized</option>
	</data>	
	
	<data id="selectKsxz">
		<option key="0">Normal Exam</option>
		<option key="1">Make-Up</option>
		<option key="2">Deferment</option>	
	</data>
	
	<!-- 申请人：审核状态下拉数据;-->
	<data id="sqrNodeList">
		<option key="WAIT_EXECUTE">To be Processed </option>
		<option key="ALREADY_EXECUTE">Processed</option>
	</data>
	
	<!-- 审核人：审核状态下拉数据;-->
	<data id="shrNodeList">
		<option key="WAIT_AUDITING">To be Reviewed</option>
		<option key="PASS_AUDITING">Pass</option>
		<option key="NO_PASS_AUDITING">No Pass</option>	
	</data>
	
	<!-- 审核人：审核状态下拉数据;-->
	<data id="shrStatusList">
		<option key="1">To be Reviewed</option>
		<option key="3">Pass</option>
		<option key="5">No Pass</option>	
	</data>
	
	<!-- 缴费状态下拉数据;-->
	<data id="payStatusList">
		<option key="1">Paid</option>
		<option key="0">Not Paid</option>
	</data>
	
	<!-- 发送邮件的类型 -->
	<data id="fslx">
		<option key="tzfs">Send Notification</option>
		<option key="cjmmfs">Send Score Password</option>
	</data>
	
	<!-- 部门类型 -->
	<data id="bmlxList">
		<option key="1">教学院系</option>
		<option key="2">科研机构</option>
		<option key="3">公共服务 指图书馆、档案馆、分析测试中心、计算/网络/信息/电教/教育技术中心等学术支撑单位</option>
		<option key="4">党务部门 含工会、团委、妇委会</option>
		<option key="5">行政机构</option>
		<option key="6">附属单位</option>
		<option key="7">后勤部门</option>
		<option key="8">校办产业</option>
		<option key="9">其他</option>
	</data>
	
	
</root>
